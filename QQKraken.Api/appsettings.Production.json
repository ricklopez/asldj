{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=#{Service.Server}#;Database=#{Service.Database}#;User Id=#{Service.User}#; Password=#{Service.Password}#;"
  },
  "AzureAd": {
    "Instance": "#{ActiveDirectory.Instance}#",
    "Tenant": "#{ActiveDirectory.Tenant}#",
    "ClientId": "#{ActiveDirectory.ClientId}#"
  }
}
